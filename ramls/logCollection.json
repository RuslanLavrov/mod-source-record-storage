{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Log record collection",
  "type": "object",
  "properties": {
    "items": {
      "description": "List of logs",
      "type": "array",
      "id": "recordList",
      "items": {
        "type": "object",
        "$ref": "log"
      }
    },
    "totalRecords": {
      "type": "integer"
    }
  },
  "required": [
    "items",
    "totalRecords"
  ]
}
